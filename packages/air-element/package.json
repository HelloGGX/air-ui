{
    "name": "@air-ui/air-element",
    "version": "0.0.0",
    "description": "air ui component library",
    "keywords": [
        "air-element",
        "element",
        "component library",
        "ui framework",
        "ui",
        "vue"
    ],
    "author": "玄子",
    "license": "MIT",
    "main": "index.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/HelloGGX/air-ui.git"
    },
    "bugs": {},
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production INPUT_DIR=components/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && pnpm run build:es && pnpm run build:lib && pnpm run build:theme && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "build:es": "vite build --config vite.es.config.ts",
        "build:lib": "vite build --config vite.lib.config.ts",
        "build:theme": "gulp --require ./node_modules/@esbuild-kit/cjs-loader"
    },
    "sideEffects": [
        "**/*.css",
        "dist/*",
        "theme/*"
    ],
    "peerDependencies": {
        "vue": "^3.2.0",
        "@air-ui/theme": "file:../theme"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "color": "^4.2.3",
        "element-plus": "^2.7.8"
    },
    "devDependencies": {
        "@esbuild-kit/cjs-loader": "^2.4.4",
        "@types/color": "^3.0.6",
        "@types/fs-extra": "^11.0.4",
        "@types/gulp": "^4.0.17",
        "@types/gulp-autoprefixer": "^0.0.37",
        "@types/gulp-postcss": "^8.0.6",
        "@types/gulp-sass": "^5.0.4",
        "consola": "^3.2.3",
        "cssnano": "^7.0.5",
        "fs-extra": "^11.2.0",
        "gulp": "^5.0.0",
        "gulp-autoprefixer": "^9.0.0",
        "gulp-postcss": "^10.0.0",
        "gulp-sass": "^5.1.0",
        "rollup-plugin-postcss": "^4.0.2",
        "sass": "^1.77.8",
        "unplugin-element-plus": "^0.8.0"
    }
}